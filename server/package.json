{
  "name": "radar",
  "version": "2.0.0",
  "description": "DataEmerge Radar server app (internal server event receiver, handler, and router)",
  "license": "MIT",
  "scripts": {
    "css": "gulp css",
    "start:watch": "nodemon",
    "start": "node dist/index.js",
    "build": "gulp transpilePaths",
    "build-all": "gulp transpilePaths && cd ../client && npm run build && cd ../server && npm run css",
    "build-logs": "gulp transpilePaths --logs true",
    "build-all-logs": "gulp transpilePaths --logs true && cd ../client && npm run build && cd ../server && npm run css",
    "build-start": "gulp transpilePaths && node dist/index.js",
    "build-all-start": "gulp transpilePaths && cd ../client && npm run build && cd ../server && npm run css && node dist/index.js",
    "build-logs-start": "gulp transpilePaths --logs true && node dist/index.js",
    "build-all-logs-start": "gulp transpilePaths --logs true && cd ../client && npm run build && cd ../server && npm run css && node dist/index.js"
  },
  "dependencies": {
    "@types/socket.io-client": "^1.4.32",
    "bcrypt-nodejs": "0.0.3",
    "colors": "^1.1.2",
    "connect-mongodb-session": "^1.4.0",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.3.1",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.2",
    "gulp-insert": "^0.5.0",
    "gulp-sass": "^3.1.0",
    "gulp-typescript": "^3.2.4",
    "gulp-util": "^3.0.8",
    "http": "0.0.0",
    "json-stable-stringify": "^1.0.1",
    "map-stream": "0.0.7",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "mongoose": "^5.0.7",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "typescript": "~2.6.2",
